<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Icons\Camera.png" />
    <None Remove="Icons\Good.png" />
    <None Remove="Icons\Heart.png" />
    <None Remove="Icons\Heart_Black.png" />
    <None Remove="Icons\Message.png" />
    <None Remove="Icons\Send.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Icons\Camera.png">
